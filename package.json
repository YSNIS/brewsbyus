    "pg-format": "^1.0.4",
    "pg-hstore": "^2.3.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.3.1",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
